    <front xml:id="front">
      <div type="section" xml:id="bibliography">
        <head>Bibliography</head>
        <!-- This is where the bibliography for the edition is listed. -->
        <div type="section" xml:id="bibliography-witnesses">
          <listWit>
            <witness xml:id="V">
              <abbr type="siglum">V</abbr>
              Codex Vaticanus Latinus 4929 ff. 188râ€“195r (<ptr target="https://digi.vatlib.it/view/MSS_Vat.lat.4929"/>, accessed on 2019-03-17).</witness>
            <witness xml:id="V1">
              <abbr type="siglum">V<hi rend="superscript">1</hi>
              </abbr>
              Corrections made by the original manuscript copyist.</witness>
            <witness xml:id="V2">
              <abbr type="siglum">V<hi rend="superscript">2</hi>
              </abbr>
              Corrections made by a hand contemporary to the original manuscript copyist.</witness>
            <witness xml:id="V3">
              <abbr type="siglum">V<hi rend="superscript">3</hi>
              </abbr>
              Corrections made by a hand at the end of the 11<hi rend="superscript">th</hi>
              century or the beginning of the 12<hi rend="superscript">th</hi>
              century.</witness>
            <witness xml:id="V4">
              <abbr type="siglum">V<hi rend="superscript">4</hi>
              </abbr>
              Corrections made by a hand a little after V<hi rend="superscript">3</hi>.</witness>
            <witness xml:id="VC">
              <abbr type="siglum">V<hi rend="superscript">c</hi>
              </abbr>
              Corrections made by a hand that cannot be identified with certainty (i.e., it could be any of those already listed).</witness>
            <witness xml:id="VP">
              <abbr type="siglum">V<hi rend="superscript">p</hi>
              </abbr>
              A reading prior to correction.</witness>
          </listWit>
          <listBibl>
            <bibl xml:id="Gelsomino">
              <editor>Gelsomino, Remus</editor>.
              <title>Vibius Sequester</title>.
              <pubPlace>Lipsiae</pubPlace>:
              <publisher>in aedibus B.G. Teubneri</publisher>. 1967.</bibl>
          </listBibl>
        </div>
      </div>
