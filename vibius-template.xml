    <body>
      <div type="edition" xml:id="edition-text">
        <!-- Beginning of section "Flumina" -->
        <div type="textpart" n="1" xml:id="part1">
          <head>Flumina</head>

          <p xml:id="crathys">
            <app>
              <lem wit="#V">
                <placeName ref="https://pleiades.stoa.org/places/452315">Crathys</placeName>, confinis
                <placeName ref="https://pleiades.stoa.org/places/452458">Sybari</placeName>. Capillos
                <lb break="no"/>facit aurei coloris</lem>
              <note target="#crathys">A note in the margin has this text:
                <lb/>capillos
                <lb/>aurei
                <lb/>colo<lb/>ris.</note>
            </app>
          </p>

          <p xml:id="clitumnus">
            <placeName ref="https://pleiades.stoa.org/places/413091">Clitumnus</placeName>,
            <placeName ref="https://pleiades.stoa.org/places/413360">Umbriae</placeName>, ubi Iuppiter
            <lb break="no"/>eodem nomine est.</p>

          <p xml:id="hypanis">
            <placeName ref="https://pleiades.stoa.org/places/825273">Hypanis</placeName>,
            <placeName ref="https://pleiades.stoa.org/places/1273">Scythiae</placeName>, qui ut ait Gal<lb break="no"/>lus uno tellures
            <app>
              <lem wit="#VC">dividi<add place="inline">t am</add>ne duas</lem>
              <rdg wit="#V">dividi neduas</rdg>
            </app>
            <lb/>
            <placeName ref="https://pleiades.stoa.org/places/981509">Asiam</placeName>
            enim ab
            <placeName ref="https://pleiades.stoa.org/places/991376">Europa</placeName>
            <app>
              <lem wit="#VC">
                <add place="inline">se</add>parat</lem>
              <rdg wit="#V">parat</rdg>
              <note target="#hypanis">A later hand has written in the margin
                <foreign xml:lang="la">Galli verba</foreign>
              </note>
            </app>
          </p>

        </div>

        <!-- End of section "Flumina" -->

        <!-- Beginning of section "Fontes" -->
        <div type="textpart" n="2" xml:id="part2">
          <head>Fontes</head>

        </div>
        <!-- End of section "Fontes" -->

        <!-- Beginning of section "Lacus" -->
        <div type="textpart" n="3" xml:id="part3">
          <head>Lacus</head>
        </div>
        <!-- End of section "Lacus" -->

        <!-- Beginnins of section "Nemora" -->
        <div type="textpart" n="4" xml:id="part4">
          <head>Nemora</head>
        </div>
        <!-- End of section "Nemora" -->

        <div type="textpart" n="5" xml:id="part5">
          <head>Paludes</head>
        </div>
        <!-- End of section "Paludes" -->

        <!-- Beginning of section "Montes" -->
        <div type="textpart" n="6" xml:id="part6">
          <head>Montes</head>
          <p xml:id="symaethos">
            <app>
              <lem source="#Gelsomino">Symaethos</lem>
              <rdg wit="#V">Sym&amp;hos</rdg>
              <rdg wit="#V1">
                <add place="margin">Symethos</add>
              </rdg>
            </app>
          </p>

          <p xml:id="ossa">
            <app>
              <lem wit="#V1" xml:id="lem-ossa">Ossa, Thessaliae</lem>
              <rdg wit="#V">Ossa, Thessaliae, ubi Hercules com<lb break="no"/>bustus est.</rdg>
              <witDetail target="#lem-ossa" wit="#V1">Entries for Oetaus and Othrys, written at the bottom of the column, are to be inserted here, as indicated by an insertion mark made by
                <ref target="#V1">V1</ref>,</witDetail>
            </app>.</p>
          <p xml:id="othrys">Othrys, Thraciae
            <app><note target="#othrys">This entry was written at the bottom of the column and marked for insertion after
              <ref target="#ossa">Ossa</ref>.</note></app>
          </p>
          <p xml:id="oeteus">
            <app>
              <lem wit="#V1" xml:id="lem-oeteus">Oeteus, Thes<lb break="no"/>saliae, ubi Hercules com<lb break="no"/>bustus est</lem>
              <witDetail wit="#V1" target="#lem-oeteus">
                <add place="margin">Oetaeus
                  <lb/>ubi her<lb/>cules com<lb/>bustus
                  <lb/>est.</add>
              </witDetail>
              <rdg wit="#V">Oeteus, Thes<lb break="no"/>saliae</rdg>
            </app>.</p>

        </div>
        <!-- End of section "Montes" -->

        <!-- Beginning of section "Gentes" -->
        <div type="textpart" n="7" xml:id="part7">
          <head>Gentes</head>
        </div>
        <!-- End of section "Gentes" -->

